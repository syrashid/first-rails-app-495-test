<h1>Meet our team</h1>

<form action="/contact" method="get">
  <input type="text" name="member" placeholder="<%= params[:member] ? params[:member] : "Who are you looking for?"%>">
  <input type="submit">
</form>

<ol>
  <% @members.each do |member| %>
    <li><%= member.capitalize %></li>
  <% end %>
</ol>
